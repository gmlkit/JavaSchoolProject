<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eAsistentMini.TeacherController">
   <children>
      <Button fx:id="addGradeButton" layoutX="460.0" layoutY="124.0" mnemonicParsing="false" onAction="#addButtonPressed" text="AddGrade" />
       <TextField fx:id="gradeField" layoutX="359.0" layoutY="14.0" promptText="Write grade here" />
      <ComboBox fx:id="studentsCombo" layoutX="24.0" layoutY="14.0" prefWidth="150.0" onContextMenuRequested="#kill"/>
      <ComboBox fx:id="classCombo" layoutX="190.0" layoutY="14.0" prefWidth="150.0" onContextMenuRequested="#me"/>
   </children>
</AnchorPane>
